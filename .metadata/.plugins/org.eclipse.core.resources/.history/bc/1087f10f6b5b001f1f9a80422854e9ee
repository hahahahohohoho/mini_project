package com.edu.road;

import com.edu.cty.City;

import lombok.Getter;

@Getter
public class RoadDTO {
	private String name;
	private String geometry;
	private String cty_name;
	private Integer grade;
	
	public RoadDTO(Road road) {
		this.name = road.getName();
		this.geometry = road.getGeometry();
		City city = road.getCty_cd();
		this.cty_name= city.getCty_nm();
		this.grade = road.getGrade();
	}
}
